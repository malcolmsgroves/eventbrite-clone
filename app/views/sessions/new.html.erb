<% provide(:title, 'Log In') %>
<div>
  <% flash.each do |message| %>
    <%= message %>
  <% end %>
  <%= link_to 'Sign Up', new_user_path %>
  <%= form_for :session, path: login_path do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.submit 'Log In' %>
  <% end %>
</div>
